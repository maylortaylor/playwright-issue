{
  "name": "amver_ui",
  "private": true,
  "version": "1.0.6",
  "author": "PoleStarGlobalDefense",
  "repository": {
    "type": "gitlab",
    "url": "https://gitlab.com/polestardefense/amver/amver-ui"
  },
  "main": "./src/main.tsx",
  "scripts": {
    "build": "webpack --progress --config webpack.prod.config.ts --mode production",
    "build:dev": "webpack --progress --config webpack.dev.config.ts --mode development",
    "clean": "rm -rf ./dist",
    "rebuild": "rm -rf node_modules && rm -rf packge-lock.json && npm cache clean --force && npm install",
    "prod": "webpack serve --progress --color --config webpack.prod.config.ts",
    "dev": "webpack serve --progress --color --config webpack.dev.config.ts",
    "devx": "cross-env EXPERIMENTAL=true webpack serve --progress --color --config webpack.dev.config.ts",
    "watch": "webpack serve --config webpack.dev.config.ts --watch --progress --color --open",
    "docker:run": "npm run build && docker build -t polestar/amver-ui . && docker run -p 8080:8080 polestar/amver-ui",
    "format:check": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "cross-env NODE_ENV=development npx playwright test -c ./playwright-ct.config.ts --trace on --grep @Smoke",
    "test:all": "cross-env NODE_ENV=development npx playwright test --config ./playwright.config.ts",
    "test:api": "cross-env NODE_ENV=development npx playwright test --config ./playwright.config.ts --grep @APIMOCK",
    "test:smoke": "cross-env NODE_ENV=development npx playwright test --config ./playwright.config.ts --grep @Smoke --project=Chrome",
    "test:smoke:ui": "cross-env NODE_ENV=development npx playwright test --config ./playwright.config.ts --grep @Smoke --ui --project=Chrome",
    "test:smoke:headless": "cross-env NODE_ENV=development npx playwright test --config ./playwright.config.ts --grep @Smoke --project=Chromium",
    "test:report": "allure serve"
  },
  "dependencies": {
    "@automapper/classes": "^8.7.7",
    "@automapper/core": "^8.7.7",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/lato": "^5.0.17",
    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^5.14.16",
    "@mui/types": "^7.2.8",
    "@mui/x-data-grid": "^6.18.0",
    "axios": "1.6.0",
    "cesium": "^1.111.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "is-plain-object": "^5.0.0",
    "jotai": "^2.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mui-datatables": "^4.3.0",
    "os-browserify": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.4.1",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^6.18.0",
    "react-use-websocket": "^4.5.0",
    "reflect-metadata": "^0.1.13",
    "resium": "^1.17.2",
    "slate": "^0.100.0",
    "slate-history": "^0.100.0",
    "slate-react": "^0.100.1",
    "ts-node": "^10.9.1",
    "tty-browserify": "^0.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.23.0",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@playwright/experimental-ct-react": "^1.39.0",
    "@playwright/test": "^1.39.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
